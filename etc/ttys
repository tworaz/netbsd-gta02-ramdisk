console	"/usr/libexec/getty default"	vt100	on  secure
constty	"/usr/libexec/getty default"	vt100	off secure
ttyS2	"/usr/libexec/getty default"	vt100	on  secure
ttyE0	"/usr/libexec/getty default"	vt100	on  secure
